<Window x:Class="Keesh.Interface.User.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Keesh.Interface.User"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Menu Grid.Row="0">
            <MenuItem Header="_View">
                <MenuItem Header="_CPI"/>
                <MenuItem Header="_GDP"/>
            </MenuItem>
        </Menu>
        <TextBlock TextAlignment="Center" Grid.Row="2">
            Data provided by 
            <Hyperlink NavigateUri="https://www.alphavantage.co/" Click="DataProviderLink_Click">https://www.alphavantage.co/</Hyperlink>
        </TextBlock>
    </Grid>
</Window>
